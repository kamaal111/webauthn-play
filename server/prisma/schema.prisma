generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  name  String
}

model PreSignUpToken {
  email         String   @unique
  token         String
  credential_id String
  created_time  DateTime
}
